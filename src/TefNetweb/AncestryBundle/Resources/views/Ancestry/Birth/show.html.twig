{% extends 'TefNetwebAncestryBundle:Default:index.html.twig' %}

    {% block  content_content -%}
        {% block headline %}
            {{ 'Ancestry Birth show'|trans }}
        {% endblock headline %}

        <ul class="record_actions">
            <li>
                <a href="{{ path('birth') }}">
                    {{ 'Back to the list'|trans }}
                </a>
            </li>
            <li>
                <a href="{{ path('birth_edit', { 'id': entity.id }) }}">
                    {{ 'Edit'|trans }}
                </a>
            </li>
        </ul>

        <table class="record_properties">
            <tbody>
            <tr>
                <th>  {{ 'Probandsurname'|trans }}</th>
                <td>{{ entity.probandSurname }}</td>
            </tr>
            <tr>
                <th>  {{ 'Probandname'|trans }}</th>
                <td>{{ entity.probandName }}</td>
            </tr>
            <tr>
                <th>  {{ 'Probandname2'|trans }}</th>
                <td>{{ entity.probandName2 }}</td>
            </tr>
            <tr>
                <th>  {{ 'Probandname3'|trans }}</th>
                <td>{{ entity.probandName3 }}</td>
            </tr>
            <tr>
                <th>  {{ 'Probandsex'|trans }}</th>
                <td>{{ trans_entity.probandSex }}</td>
            </tr>
            <tr>
                <th>  {{ 'Probandbirthexitus'|trans }}</th>
                <td>{{ trans_entity.probandBirthExitus }}</td>
            </tr>
            <tr>
                <th>  {{ 'Fathersurname'|trans }}</th>
                <td>{{ entity.fatherSurname }}</td>
            </tr>
            <tr>
                <th>  {{ 'Fathername'|trans }}</th>
                <td>{{ entity.fatherName }}</td>
            </tr>
            <tr>
                <th>  {{ 'Fathername2'|trans }}</th>
                <td>{{ entity.fatherName2 }}</td>
            </tr>
            <tr>
                <th>  {{ 'Fathername3'|trans }}</th>
                <td>{{ entity.fatherName3 }}</td>
            </tr>
            <tr>
                <th>  {{ 'Fatherage'|trans }}</th>
                <td>{{ entity.fatherAge }}</td>
            </tr>
            <tr>
                <th>  {{ 'Fatherstatus'|trans }}</th>
                <td>{{ trans_entity.fatherStatus }}</td>
            </tr>
            <tr>
                <th>  {{ 'Fatherprofession'|trans }}</th>
                <td>{{ entity.fatherProfession }}</td>
            </tr>
            <tr>
                <th>  {{ 'Mothersurname'|trans }}</th>
                <td>{{ entity.motherSurname }}</td>
            </tr>
            <tr>
                <th>  {{ 'Mothername'|trans }}</th>
                <td>{{ entity.motherName }}</td>
            </tr>
            <tr>
                <th>  {{ 'Mothername2'|trans }}</th>
                <td>{{ entity.motherName2 }}</td>
            </tr>
            <tr>
                <th>  {{ 'Mothername3'|trans }}</th>
                <td>{{ entity.motherName3 }}</td>
            </tr>
            <tr>
                <th>  {{ 'Motherage'|trans }}</th>
                <td>{{ entity.motherAge }}</td>
            </tr>
            <tr>
                <th>  {{ 'Motherstatus'|trans }}</th>
                <td>{{ trans_entity.motherStatus }}</td>
            </tr>
            <tr>
                <th>  {{ 'Motherprofession'|trans }}</th>
                <td>{{ entity.motherProfession }}</td>
            </tr>
            <tr>
                <th>  {{ 'Birthdate'|trans }}</th>
                <td>
                    {% if entity.birthDate != null %}
                        {{ entity.birthDate|date('Y-m-d') }}
                    {% endif %}
                </td>
            </tr>
            <tr>
                <th>  {{ 'Birthtime'|trans }}</th>
                <td>
                    {% if entity.birthTime != null %}
                        {{ entity.birthTime|date('H:i') }}
                    {% endif %}
                </td>
            </tr>
            <tr>
                <th>  {{ 'Registrationdate'|trans }}</th>
                <td>
                    {% if entity.registrationDate != null %}
                        {{ entity.registrationDate|date('Y-m-d') }}
                    {% endif %}
                </td>
            </tr>
            <tr>
                <th>  {{ 'Birthplace'|trans }}</th>
                <td>{{ entity.birthPlace }}</td>
            </tr>
            <tr>
                <th>  {{ 'Birthparish'|trans }}</th>
                <td>{{ entity.birthParish }}</td>
            </tr>
            <tr>
                <th>  {{ 'Residence'|trans }}</th>
                <td>{{ entity.residence }}</td>
            </tr>
            <tr>
                <th>  {{ 'Residenceparish'|trans }}</th>
                <td>{{ entity.residenceParish }}</td>
            </tr>
            <tr>
                <th>  {{ 'Notes'|trans }}</th>
                <td>{{ entity.notes }}</td>
            </tr>
            </tbody>
        </table>

        <ul class="record_actions">
            <li>
                <a href="{{ path('birth') }}">
                    {{ 'Back to the list'|trans }}
                </a>
            </li>
            <li>
                <a href="{{ path('birth_edit', { 'id': entity.id }) }}">
                    {{ 'Edit'|trans }}
                </a>
            </li>
            <li>{{ form(delete_form) }}</li>
        </ul>
    {% endblock %}
